{
  "name": "jm-castle-warehouse-server",
  "version": "0.6.3",
  "type": "module",
  "module": "./build",
  "files": [
    "build",
    "src"
  ],
  "exports": {
    "./build": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    },
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "private": false,
  "scripts": {
    "prebuild": "eslint \"./src/**\"",
    "build": "tsc --project tsconfig.json",
    "buildmodule": "rollup -c",
    "start": "node --experimental-modules ./build/start.mjs",
    "dev": "nodemon --ext '*.mts' -x \"npm run build && npm run start \"",
    "dev:serve": "cross-env NODE_ENV=development ts-node-dev --inspect=9229 --nolazy --respawn ./src -r ts-node/register src/."
  },
  "peerDependencies": {
    "jm-castle-warehouse-types": "^0.x"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "express": "~4.16.1",
    "form-data": "^4.0.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^3.0.4",
    "mariadb": "^3.0.1",
    "morgan": "~1.9.1",
    "multiparty": "^4.2.3",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.8.0",
    "sharp": "^0.31.2",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@rbnlffl/rollup-plugin-eslint": "^3.0.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.2",
    "@types/luxon": "^3.0.1",
    "@types/morgan": "1.9.3",
    "@types/multiparty": "^0.0.33",
    "@types/nodemailer": "^6.4.6",
    "@types/sharp": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "jm-castle-warehouse-types": "^0.7.9",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.22",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
