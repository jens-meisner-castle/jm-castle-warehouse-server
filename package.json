{
  "name": "jm-castle-warehouse-server",
  "version": "1.3.0",
  "type": "module",
  "module": "./build",
  "files": [
    "build",
    "src"
  ],
  "exports": {
    "./build": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    },
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "private": false,
  "scripts": {
    "clean": "del-cli build",
    "prebuild": "npm run clean && eslint \"./src/**\"",
    "build": "tsc --project tsconfig.json",
    "start": "node ./build/start.mjs",
    "dev": "nodemon --ext '*.mts' -x \"npm run build && npm run start \"",
    "dev:serve": "cross-env NODE_ENV=development ts-node-dev --inspect=9229 --nolazy --respawn ./src -r ts-node/register src/."
  },
  "peerDependencies": {
    "jm-castle-mariadb": "^0.x",
    "jm-castle-types": "^0.x",
    "jm-castle-warehouse-types": "^0.x"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "~4.16.1",
    "form-data": "^4.0.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^3.0.4",
    "mariadb": "^3.0.1",
    "multiparty": "^4.2.3",
    "node-fetch": "^3.2.10",
    "node-stream-zip": "^1.15.0",
    "nodemailer": "^6.8.0",
    "sharp": "^0.31.2",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/luxon": "^3.0.1",
    "@types/multiparty": "^0.0.33",
    "@types/nodemailer": "^6.4.6",
    "@types/sharp": "^0.31.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.1",
    "jm-castle-mariadb": "^0.3.0",
    "jm-castle-types": "^0.4.0",
    "jm-castle-warehouse-types": "^0.17.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
